{
  "name": "prj_node_first",
  "version": "1.0.0",
  "description": "Project node introduction",
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seeders/seed.ts"
  },
  "scripts": {
    "lint": "eslint src --ext js --report-unused-disable-directives --max-warnings 0",
    "prod": "npm run build && cross-env NODE_ENV=prod node ./build/src/server.js",
    "build": "tsc && tsc-alias",
    "dev": "cross-env NODE_ENV=dev nodemon --exec ts-node -r tsconfig-paths/register ./src/server.ts"
  },
  "author": "TDev",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.10",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "path": "^0.12.7",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/cors": "^2.8.15",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.5.2"
  }
}
